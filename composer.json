{
  "name": "kagg/pagespeed-module",
  "description": "PageSpeed Module",
  "license": "GPL-2.0",
  "keywords": [
    "PageSpeed Module",
    "Mod PageSpeed",
    "ModPageSpeed",
    "Apache",
    "Nginx",
    "cache"
  ],
  "homepage": "https://kagg.eu/en/",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "support": {
    "issues": "https://wordpress.org/support/plugin/kagg-pagespeed-module/"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.9.0",
    "phpcompatibility/php-compatibility": "^9.3.5",
    "phpcompatibility/phpcompatibility-wp": "^2.1.4",
    "wp-coding-standards/wpcs": "^3.0.1"
  },
  "autoload": {
    "psr-4": {
      "KAGG\\PagespeedModule\\": "src/php"
    }
  },
  "scripts": {
    "phpcs": "phpcs --colors"
  }
}
