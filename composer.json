{
  "name": "kagg/pagespeed-module",
  "description": "PageSpeed Module",
  "license": "GPL-2.0",
  "keywords": [
    "PageSpeed Module",
    "Mod PageSpeed",
    "mod_pagespeed",
    "Apache",
    "Nginx",
    "cache"
  ],
  "homepage": "https://kagg.eu/en/",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "support": {
    "issues": "https://wordpress.org/support/plugin/kagg-pagespeed-module/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "5.6"
    },
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.6",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "wp-coding-standards/wpcs": "^2.3"
  },
  "autoload": {
    "classmap": [
      "classes"
    ]
  },
  "scripts": {
    "phpcs": "phpcs --colors"
  }
}
